<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.przemek.egzaminel">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_USER_DICTIONARY" />

    <application
        android:name="com.example.przemek.egzaminel.Network.AppNetworkController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.przemek.egzaminel._Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.przemek.egzaminel._Activities.Exam.ExamActivity" />
        <activity android:name="com.example.przemek.egzaminel._Activities.DataVies.GroupsActivity" />
        <activity android:name="com.example.przemek.egzaminel._Activities.DataVies.ExamsActivity" />
        <activity android:name="com.example.przemek.egzaminel._Activities.MainMenu" />
        <activity android:name="com.example.przemek.egzaminel._Activities.Calendar.CalendarActivity"></activity>
    </application>

</manifest>